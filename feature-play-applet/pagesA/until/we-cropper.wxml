<template name="we-cropper">
    <canvas bindtouchend="touchEnd" bindtouchmove="touchMove" bindtouchstart="touchStart" canvasId="{{id}}" class="cropper" disableScroll="true" style="width:{{width}}px;height:{{height}}px;background-color: #fff;"></canvas>
    <canvas canvasId="{{targetId}}" class="cropper" disableScroll="true" style="position: fixed; top: -{{width*pixelRatio}}px; left: -{{height*pixelRatio}}px; width:{{width*pixelRatio}}px;height:{{height*pixelRatio}}px;"></canvas>
</template>
